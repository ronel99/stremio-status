      "pluginVersion": "10.4.0",
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "P51658C9617260799"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH relevant_data AS (\n  SELECT * FROM endpoint_results WHERE $__timeFilter(timestamp)\n),\nflaps_data AS (\n  SELECT endpoint_id, COUNT(*) as cnt\n  FROM (\n    SELECT endpoint_id, success, LAG(success) OVER (PARTITION BY endpoint_id ORDER BY timestamp) as prev\n    FROM relevant_data\n  ) t\n  WHERE prev IS NOT NULL AND success != prev\n  GROUP BY endpoint_id\n),\nstats_data AS (\n  SELECT endpoint_id, avg(success::int)*100 as uptime, avg(duration/1000000.0) as latency\n  FROM relevant_data\n  GROUP BY endpoint_id\n)\nSELECT\n  e.endpoint_name as \"Instance\",\n  e.endpoint_group as \"Group\",\n  s.uptime as \"Uptime %\",\n  s.latency as \"Avg Latency\",\n  COALESCE(f.cnt, 0) as \"Flaps\"\nFROM endpoints e\nJOIN stats_data s ON e.endpoint_id = s.endpoint_id\nLEFT JOIN flaps_data f ON e.endpoint_id = f.endpoint_id\nORDER BY 3 DESC, 4 ASC",
          "refId": "A"
        }
      ],
      "title": "Global Health Leaderboard (Uptime & Stability)",
      "type": "table"
    },
